terraform {
  required_providers {
    aws = {
      source = "hashicorp/aws"
    }
  }
}
provider "aws" {
  region = "us-east-1"
}

resource "aws_lambda_function" "myLambda" {
  function_name = "MyFirstFunction"

  
  s3_bucket = "https://us-east-1.console.aws.amazon.com/s3/buckets/cf-templates-904bfglshlve-us-east-1?region=us-east-1"
  s3_key    = "hello.zip"

  
  handler = "hello.handler"
  runtime = "nodejs12.x"

  role = "myFirstFunction-role-dthno85t"
}

{
  "Version": "2012-10-17",
